{
  "name": "automates-screening-job-application",
  "main": "src/start.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/start.ts",
    "init": "bun run src/init.ts",
    "dev": "bun run --watch src/start.ts",
    "build": "bun build src/start.ts --outdir ./dist --target bun",
    "prod": "NODE_ENV=production bun run src/start.ts",
    "lint": "biome lint .",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs job-evaluation-app",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:start:prod": "pm2 start ecosystem.config.cjs --env production",
    "pm2:restart:prod": "pm2 restart ecosystem.config.cjs --env production",
    "app:start": "./scripts/start.sh",
    "app:stop": "./scripts/stop.sh",
    "app:restart": "./scripts/restart.sh",
    "setup:pm2": "./scripts/setup-pm2.sh"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/bun": "latest",
    "@types/pg": "^8.15.5"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@chroma-core/google-gemini": "^0.1.7",
    "@google/genai": "^1.25.0",
    "@hono/node-server": "^1.19.5",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.3",
    "@hono/zod-validator": "^0.7.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^11.0.0",
    "better-auth": "^1.3.27",
    "bun-types": "^1.3.0",
    "chromadb": "^3.0.17",
    "express-rate-limit": "^8.1.0",
    "form-data": "^4.0.4",
    "hono": "^4.10.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^2.4.3",
    "pg": "^8.16.3",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  }
